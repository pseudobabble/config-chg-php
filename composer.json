{
    "name": "dividohq/config-chg-php",
    "type": "project",
    "description": "@dividohq coding challenge for PHP candidates (config-related)",
    "license": "proprietary",
    "authors": [
        {
            "name": "Steve Hill",
            "email": "steve.hill@divido.com"
        }
    ],
    "require": {
        "php": "^7.4",
        "ext-json": "*",
        "symfony/yaml": "^5.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.2"
    },
    "config": {
        "bin-dir": "bin/"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/",
            "App\\Tests\\ExtraCredit\\": "tests/extra_credit/"
        }
    },
    "minimum-stability": "stable"
}
